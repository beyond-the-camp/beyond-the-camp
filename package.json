{
  "name": "beyond-the-camp",
  "description": "Beyond the Camp",
  "version": "0.0.1",
  "author": "Hentie Louw <hentie.louw@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "rimraf .cache public",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore . --ext js --ext jsx --ext ts --ext tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "gatsby": "^2.11.7",
    "gatsby-image": "^2.2.3",
    "gatsby-plugin-google-analytics": "^2.1.1",
    "gatsby-plugin-netlify": "^2.1.0",
    "gatsby-plugin-netlify-cms": "^4.1.1",
    "gatsby-plugin-postcss": "^2.1.0",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-react-leaflet": "2.0.11",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.2",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.1.1",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "leaflet": "^1.5.1",
    "netlify-cms-app": "^2.9.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.9.0",
    "react-leaflet": "^2.4.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.22",
    "@types/react-helmet": "5.0.8",
    "@types/react-intl": "2.3.18",
    "@types/react-leaflet": "2.2.1",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "autoprefixer": "9.6.0",
    "babel-eslint": "10.0.2",
    "eslint-config-prettier": "6.0.0",
    "eslint-loader": "2.2.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-react": "7.14.2",
    "gatsby-plugin-eslint": "2.0.5",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.0.2",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.18.2",
    "react-testing-library": "8.0.1",
    "rimraf": "2.6.3",
    "tailwindcss": "1.0.4",
    "ts-jest": "24.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --ignore-path .gitignore --ext ts --ext tsx --ext js --ext jsx --fix",
      "git add"
    ]
  }
}
