{
  "name": "beyond-the-camp",
  "description": "Beyond the Camp",
  "version": "0.0.1",
  "author": "Hentie Louw <hentie.louw@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "rimraf .cache public",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore . --ext js --ext jsx --ext ts --ext tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "gatsby": "^2.2.9",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-netlify-cms": "^3.0.16",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-purgecss": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-react-leaflet": "2.0.4",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "leaflet": "^1.4.0",
    "netlify-cms": "^2.8.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "react-leaflet": "^2.2.1",
    "react-typography": "^0.16.19",
    "sharp": "^0.22.0",
    "typescript": "^3.3.4000",
    "typography": "^0.16.19",
    "typography-theme-noriega": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-helmet": "^5.0.8",
    "@types/react-intl": "^2.3.17",
    "@types/react-leaflet": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-eslint": "^2.0.4",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.2",
    "rimraf": "^2.6.3",
    "tailwindcss": "^0.7.4",
    "ts-jest": "^24.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "src/**/*.{ts,tsx,js,json}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.{ts,tsx,js,jsx}": [
        "eslint --ignore-path .gitignore --ext ts --ext tsx --ext js --ext jsx --fix",
        "git add"
      ]
    }
  }
}
